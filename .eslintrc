rules:
    indent: 4
    brace-style: [2, "1tbs"]
    comma-style: [2, "last"]
    default-case: 2
    func-style: [2, "declaration"]
    guard-for-in: 2
    no-floating-decimal: 2
    no-nested-ternary: 2
    no-undefined: 2
    quotes: "single"
    radix: 2
    space-after-keywords: [2, "always"]
    space-before-blocks: 2
    spaced-line-comment: [2, "always", { exceptions: ["-"]}]
    strict: [2, "global"]
    global-strict: [2, "always"]
    valid-jsdoc: [2, { prefer: { "return": "returns"}}]
    wrap-iife: [2, "inside"]